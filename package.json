{
  "name": "outtask",
  "id": "outtask",
  "version": "0.1.0",
  "description": "Library for describing async operations as data & managing them as explicit effects",
  "keywords": [
    "task",
    "type",
    "flow",
    "promise",
    "future",
    "effects"
  ],
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "homepage": "https://github.com/Gozala/outtask",
  "main": "./lib/Task.js",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "flow-bin": "^0.43.1",
    "flow-copy-source": "1.1.0",
    "standard-flow": "1.0.0",
    "tape": "^4.6.3"
  },
  "scripts": {
    "test": "npm run flow && npm run lint && npm run tape",
    "tape": "tape -r babel-register test/**/*.js",
    "flow": "flow check",
    "lint": "standard-flow",
    "start": "babel --watch --out-dir lib src",
    "copy-types": "flow-copy-source -v src lib",
    "build-node": "babel --out-dir lib src",
    "build": "npm run build-node && npm run copy-types",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gozala/outtask.git",
    "web": "https://github.com/Gozala/outtask"
  },
  "bugs": {
    "url": "http://github.com/Gozala/outtask/issues/"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "preemptive-animation-frame": "^0.1.0"
  }
}
